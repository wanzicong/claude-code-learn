{
    "vue": {
        "command": "vue-language-server",
        "args": ["--stdio"],
        "extensionToLanguage": {
            ".vue": "vue"
        },
        "transport": "stdio",
        "initializationOptions": {
            "typescript": {
                "tsdk": "node_modules/typescript/lib"
            },
            "vue": {
                "hybridMode": false
            }
        },
        "settings": {},
        "maxRestarts": 3
    }
}
